{
  "name": "fastify-mongoose-demo",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc -w",
    "dev": "nodemon",
    "start": "node build/server.js",
    "test-unit": "ENV=test tap test/unit/**/*.test.ts --ts --no-coverage",
    "test-integration": "ENV=test tap test/integration/**/*.test.ts --ts --no-coverage",
    "test": "ENV=test tap test/**/*.test.ts --ts --no-coverage",
    "test-filter": "ENV=test tap --grep=$npm_config_filter --ts --no-coverage",
    "test-coverage": "ENV=test tap test/**/*.test.ts --ts",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "husky-prepare": "husky install",
    "husky-generate-pre-commit": "npx husky add .husky/pre-commit \"make format && make lint-and-fix\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.6.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/swagger": "^8.2.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@sinclair/typebox": "^0.25.12",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.3.0",
    "fastify-swagger": "^5.2.0",
    "mongoose": "^6.7.3",
    "nodemon": "^2.0.20",
    "tap": "^16.3.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.9",
    "@types/pino": "^7.0.5",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
